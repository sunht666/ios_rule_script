name: "小红书添加语音按钮"
desc: "小红书添加语音按钮"

script-providers:
  XHS-JS:
    url: https://raw.githubusercontent.com/sunht666/ios_rule_script/master/script/sebastian/redbook/xhs.js
    interval: 86400

http:
  script:
    # 正则使用单引号，无需双反斜杠，看着清爽
    - match: '^https://edith\.xiaohongshu\.com/api/sns/v1/system/cold_start_config'
      name: XHS-JS 
      type: http-response
      requires-body: true
      timeout: 30
  mitm:
    hostnames:
      - edith.xiaohongshu.com
